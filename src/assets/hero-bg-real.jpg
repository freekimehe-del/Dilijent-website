                <div class="row">



                <div class="col-12 col-sm-6">

                <div class="field-wrap first-block">

                    <select name="i-am" id="i-am" class="form-select form-control not-empty" required>
                            <option value=""></option>
                    </select>
                        <label for="i-am" class="form-label float-label">Interested In</label>
                        <label class="float-error">This field is required</label>
                    </div>

                </div>
                <div class="col-12 col-sm-6" id="rcm-profession-block">

                <div class="field-wrap">

                <select name="profession" id="profession" class="form-select form-control not-empty" required>
                    
                    <option value=""></option>
                    <option value="Physician">Physician</option>
                    <option value="Healthcare Provider">Healthcare Provider</option>
                    <option value="Practice Staff">Practice Staff</option>
                    <option value="Laboratory Staff">Laboratory Staff</option>
                    <option value="Consultant">Consultant</option>
                    <option value="Hospital Staff">Hospital Staff</option>
                    <option value="Others">Others</option>
                </select>

                <label for="profession" class="form-label float-label">I am</label>
                <label class="float-error">This field is required</label>

                </div>

                </div>

                </div>


                    <div class="field-wrap">
                        <input type="text" name="company-name" id="company-name" class="form-control">
                        <label for="company-name" class="form-label float-label">Company Name</label>
                        <label class="float-error">This field is required</label>
                    </div>

                    <div class="row">
                        <div class="col-12 col-sm-6">
                            <div class="field-wrap">
                                <input type="text" name="first-name" id="first-name" class="form-control" required>
                                <label for="first-name" class="form-label float-label">First Name</label>
                                <label class="float-error">default fname</label>
                            </div>
                        </div>

                        <div class="col-12 col-sm-6">
                            <div class="field-wrap">
                                <input type="text" name="last-name" id="last-name" class="form-control" required>
                                <label for="last-name" class="form-label float-label">Last Name</label>
                                <label class="float-error">This field is required</label>
                            </div>
                        </div>
                    </div>

                    <div class="field-wrap">
                        <input type="text" name="email" id="email" class="form-control error" required>
                        <label for="email" class="form-label float-label">Email</label>
                        <label class="float-error"></label>
                    </div>


                    <div class="row">

                        <div class="col-12 col-sm-6">

                            <div class="field-wrap">
                                <input type="tel" name="telephone" id="telephone" class="form-control" required>
                                <label for="telephone" class="form-label float-label">Telephone</label>
                                <label class="float-error">This field is required</label>
                            </div>

                        </div>

                        <div class="col-12 col-sm-6">

                            <div class="field-wrap">
                                <input type="tel" name="workphone" id="workphone" class="form-control">
                                <label for="workphone" class="form-label float-label">Work Phone</label>
                                <label class="float-error">This field is required</label>
                            </div>

                        </div>

                    </div>


                    <div class="field-wrap">
                        <label for="comments" class="form-label comments-label">Comments</label>
                        <textarea name="comments" id="comments" class="form-control" rows="2" placeholder="Your comments here..."></textarea>
                        <label class="float-error">Please add your message here.</label>
                    </div>
                    <div class="mt-2">
                        <div class="d-flex justify-content-center">
                            <button type="submit" class="request-form-submit">Send Request</button>
                        </div>
                    </div>
                </form>
            </div>

            <script>


            const pathSegments = window.location.pathname.split('/').filter(Boolean);
            let serviceName = null;

            if (pathSegments[0] === 'services' && pathSegments[1]) {
                serviceName = pathSegments[1];
            }

            console.log('SERVICE NAME', serviceName);


                const validateEmail2 = (email) => {
                    return String(email)
                        .toLowerCase()
                        .match(
                            /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|.(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/
                        );
                };

                const containsOnlyNumbers = (inputString) => {
                    // Use a regular expression to check if the string contains only numbers
                    return /^\d+$/.test(inputString);
                }

                const requestForm = document.getElementById('require-callbackform');

                if (requestForm) {

                    console.log('inside a request form');

                    let dataSection;

                    var subField = null;
                    var triggerOrigin = null;
                    let closeFormModal = document.getElementById("close-form-modal");

                    closeFormModal.addEventListener('click', e => {
                        e.preventDefault();
                        document.body.classList.remove("modal-form-open");
                    });

                    let servicesCTA = document.querySelectorAll('.service-cta');
                    let rcmPressionBlock = document.getElementById('rcm-profession-block');

                    const handleServiceCTAEvent = (e) => {

                        e.preventDefault();
                        let targetAnchor = e.target.closest('a');
                        subField = targetAnchor.getAttribute('data-subfield');
                        triggerOrigin = targetAnchor.getAttribute('data-origin');

                        dataSection = targetAnchor.getAttribute('data-service');

                        if(dataSection == 'RCM') {
                            rcmPressionBlock.style.display = 'block';
                        }
                        else {
                            rcmPressionBlock.style.display = 'none';
                        }

                        console.log('triggerOrigin', triggerOrigin);

                        if(true)
                        {

                            /* MEETING MODAL */
                            console.log('data-section', dataSection);
                            document.body.classList.remove('meeting-modal-open');
                            var dropSelect = document.querySelector("select[name='i-am']");

                            if(dataSection == 'RCM') {

                                console.log('rmc drops active');

                                dropSelect.innerHTML = "";
                                var rmcOptions = [
                                { value: "", text: "" },
                                { value: "RCM", text: "Revenue Cycle Management" },
                                { value: "Medical Billing", text: "Medical Billing" },
                                { value: "Medical Coding", text: "Medical Coding" },
                                { value: "Medical Credentialing", text: "Medical Credentialing" },
                                { value: "Denial Management Services", text: "Denial Management Services" },
                                { value: "Medical Billing Audit", text: "Medical Billing Audit" },
                                { value: "Account Receivable Management", text: "Account Receivable Management"},
                                { value: "Home Health & Hospice Billing", text: "Home Health & Hospice Billing"},
                                { value: "CONSULTATION", text: "Consultation"},
                                { value: "Others ", text: "Other" }

                                ];

                                rmcOptions.forEach(optionData => {
                                    var option = document.createElement("option");
                                    option.value = optionData.value;
                                    option.textContent = optionData.text;
                                    dropSelect.appendChild(option);
                                });
                                
                            }

                            else if(dataSection == 'ITS') {

                                console.log('IT drops active');

                                dropSelect.innerHTML = "";

                               var itOptions; 

                                if(serviceName === null || serviceName == 'software-development-services') {

                                    itOptions = [
                                { value: "", text: "" },
                                { value: "Mobile App Development", text: "Mobile App Development" },
                                { value: "SEO", text: "SEO" },
                                { value: "Cloud Computing", text: "Cloud Computing" },

                                { value: "AI/ML Applications", text: "AI/ML Applications" },
                                { value: "Business Intelligence", text: "Business Intelligence" },                            
                                { value: "Video Animation", text: "Video Animation" },


                                { value: "Web Development", text: "Web Development" },
                                { value: "E-Commerce Development", text: "E-Commerce Development" },
                                { value: "Robotic Automation", text: "Robotic Automation" },

                                { value: "Biotech Software", text: "Biotech Software" },
                                { value: "Quantum Computing", text: "Quantum Computing" },

                                { value: "Constulation", text: "Consultation" },
                                { value: "Others", text: "Others" }
                                ];

                                }
                                else {

                                    itOptions = [

                                        { value: "", text: "" },
                                        { value: "Have a Project to Discuss", text: "Have a Project to Discuss" },
                                        { value: "Consultation", text: "Consultation" },
                                        { value: "Others", text: "Others" },

                                    ];

                                }
                                
                                

                                itOptions.forEach(optionData => {
                                    var option = document.createElement("option");
                                    option.value = optionData.value;
                                    option.textContent = optionData.text;
                                    dropSelect.appendChild(option);
                                });
                            }                          
                        }

                        console.log("subfield", subField);
                        document.body.classList.add("modal-form-open");
                    }


                    servicesCTA.forEach((servieItemCTA, index) => {

                        console.log(servieItemCTA, index);

                        servieItemCTA.addEventListener("click", handleServiceCTAEvent);

                    });


                    /*field variables*/
                    let selectValueform = document.getElementById('i-am');
                    let professionSelect = document.getElementById('profession');
                    var inputCompanyName = document.getElementById('company-name');
                    var inputFirstName = document.getElementById('first-name');
                    var inputLastName = document.getElementById('last-name');
                    var inputEmail = document.getElementById('email');
                    var inputTelephone = document.getElementById('telephone');
                    var inputWorkPhone = document.getElementById('workphone');
                    var inputComments = document.getElementById('comments');
                    var error = false;

                    selectValueform.addEventListener('change', function(e) {
                        console.log('cating change event of the select field', selectValueform.value);
                        if (selectValueform.value == "") {
                            selectValueform.classList.remove('not-empty');
                            console.log('is empty');
                        } else {
                            selectValueform.classList.add('not-empty');
                            console.log('not empty');
                            selectValueform.classList.remove('error');
                        }
                    });

                    requestForm.addEventListener('submit', function(e) {

                        error = false;
                        e.preventDefault();
                        console.log('dataSection inside submit', dataSection);
                        console.log('start submission error status', error);

                        let formData = new FormData(requestForm);
                        var iam = formData.get('i-am');
                        var profession = formData.get('profession');
                        var company = formData.get('company-name');
                        var firstName = formData.get('first-name');
                        var lastName = formData.get('last-name');
                        var email = formData.get('email');
                        var telephone = formData.get('telephone');
                        var workPhone = formData.get('workphone');
                        var message = formData.get('comments');
                        var serviceType = formData.get('serviceType');
                        serviceType = dataSection;

                        if(serviceName != null) {
                            subField = serviceName; 
                        }


                        var payload = {
                            serviceType,
                            subField,
                            iam,
                            company,
                            firstName,
                            lastName,
                            email,
                            telephone,
                            workPhone,
                            message
                        };

                        if (iam == "" || iam.length == 0) {
                            error = true;
                            selectValueform.classList.add('error');
                            document.querySelector("#i-am ~ .float-error").textContent = "Choose your area of expertise";
                        }

                        if(dataSection == 'RCM') {
                            if(profession =="" || profession.length == 0) {
                                document.querySelector("#profession ~ .float-error").textContent = "Choose your profession";
                            }
                            payload.profession = profession;                          
                        }

                        if (company != "" && company.length < 3) {
                            error = true;
                            inputCompanyName.classList.add('error');
                            document.querySelector("#company-name ~ .float-error").textContent = "min. 3 characters";
                        }
                        else {
                            inputCompanyName.classList.remove('error');
                        }

                        if (firstName == "") {
                            error = true;
                            inputFirstName.classList.add('error');
                            document.querySelector("#first-name ~ .float-error").textContent = "Firstname is required";
                        }

                        if (firstName.length < 3 && firstName != "") {
                            error = true;
                            inputFirstName.classList.add('error');
                            document.querySelector("#first-name ~ .float-error").textContent = "min. 3 characters";
                        }

                        if (firstName.length >= 3 && firstName != "") {
                            inputFirstName.classList.remove('error');
                        }


                        if (lastName == "") {
                            error = true;
                            inputLastName.classList.add('error');
                            document.querySelector("#last-name ~ .float-error").textContent = "required";
                        } else if (lastName.length < 3) {
                            error = true;
                            inputFirstName.classList.add('error');
                            document.querySelector("#last-name ~ .float-error").textContent = "min. 3 characters";
                        } else {
                            error = false;
                            inputLastName.classList.remove('error');
                        }

                        if (email.length == 0) {
                            console.log("email not provided hit");
                            document.querySelector("#email ~ .float-error").textContent = "required";
                            inputEmail.classList.add("error");
                            console.log('input email', inputEmail);
                            error = true;
                        } else if (!validateEmail2(email)) {

                            document.querySelector("#email ~ .float-error").textContent = "Invalid Email";
                            inputEmail.classList.add('error');
                            error = true
                        } else if (validateEmail2(email)) {
                            document.querySelector("#email ~ .float-error").textContent = "";
                            inputEmail.classList.remove('error');
                        }

                        if (telephone == "") {
                            document.querySelector("#telephone ~ .float-error").textContent = "required";
                            inputTelephone.classList.add('error');
                            console.log('tel was empty');
                            error = true;
                        } else if (telephone.length < 6) {
                            document.querySelector("#telephone ~ .float-error").textContent = "min. 6 characters";
                            inputTelephone.classList.add('error');
                            error = true;
                            console.log('tel lenght less then 6');
                        } else if (!containsOnlyNumbers(telephone)) {

                            document.querySelector("#telephone ~ .float-error").textContent = "only numbers are allowed";
                            inputTelephone.classList.add('error');
                            error = true;
                            console.log('tel not pure numbers');

                        } else {
                            inputTelephone.classList.remove('error');
                        }


                        if (workPhone != "" && workPhone.length < 6) {
                            document.querySelector("#workphone ~ .float-error").textContent = "min. 6 characters";
                            inputWorkPhone.classList.add('error');
                            error = true;
                        } else if (workPhone != "" && !containsOnlyNumbers(workPhone)) {
                            document.querySelector("#workphone ~ .float-error").textContent = "only numbers are allowed";
                            inputWorkPhone.classList.add('error');
                            error = true;
                        } else {
                            inputWorkPhone.classList.remove('error');
                        }

                        if (message == "") {
                            inputComments.classList.add('error');
                            error = true;
                        } else {
                            inputComments.classList.remove('error');
                        }

                        if (error) {
                            console.log('error seen on the form');
                            return false;
                        }

                        console.log('this message must not bee seen post error encounter');

                        let dataPayload = JSON.stringify(payload);


                        showAlertMessage('info', "Please wait while we are processing your request.");


                        (async () => {
                            const rawResponse = await fetch('/api/modal-form', {
                                method: 'POST',
                                headers: {
                                    'Accept': 'application/json',
                                    'Content-Type': 'application/json'
                                },
                                body: dataPayload
                            });


                            const content = await rawResponse.json();

                            if (content.status == 'Success') {
                                showAlertMessage('success', content.message);
                                requestForm.reset();
                                document.body.classList.remove("modal-form-open");

                            } else {
                                showAlertMessage('error', content.message);
                            }


                        })();
                        


                    }); /* end sending */

                }
            </script>
